     System.out.print("Masukkan Batas Nilai Awal : ");
     genapawal = input.nextInt();
     System.out.print("Masukkan Batas Nilai Akhir : ");
     genapakhir = input.nextInt();
        
        for(int i=1; i<=20;i++)
            if (i % 2 == 0){
                System.out.print(i + " ");
            }
    }
    
